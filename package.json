{
  "name": "llm-dev-bench",
  "version": "1.0.0",
  "description": "Benchmark that measures cost-efficiency of LLMs on real software development tasks",
  "main": "dist/cli.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/cli.js",
    "prebench": "npm run build",
    "bench": "node dist/cli.js run",
    "prereport": "npm run build",
    "report": "node dist/cli.js report",
    "dev": "ts-node -r tsconfig-paths/register src/cli.ts",
    "setup": "bash scripts/setup-fixtures.sh",
    "docker:build": "docker build -t llm-dev-bench .",
    "docker:bench": "bash scripts/docker-bench.sh",
    "docker:merge": "bash scripts/merge-results.sh",
    "docker:build:fullstack": "docker build -f docker/fullstack/Dockerfile -t llm-fullstack-bench .",
    "docker:fullstack": "bash docker/fullstack/scripts/run-single.sh",
    "docker:bench:fullstack": "bash docker/fullstack/scripts/run-benchmark.sh claude-code,gemini-cli,codex-cli j26,j27,j28,j29,j30,j31 3 3",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "bin": {
    "llm-dev-bench": "dist/cli.js"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.36.0",
    "@anthropic-ai/vertex-sdk": "^0.4.0",
    "@google/generative-ai": "^0.21.0",
    "chart.js": "^4.5.1",
    "chartjs-node-canvas": "^5.0.0",
    "commander": "^12.1.0",
    "dotenv": "^16.4.5",
    "js-yaml": "^4.1.0",
    "openai": "^4.77.0",
    "p-limit": "^3.1.0",
    "pdfkit": "^0.17.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.10.0",
    "@types/pdfkit": "^0.17.5",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.2"
  }
}
