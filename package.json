{
  "name": "llm-dev-bench",
  "version": "1.0.0",
  "description": "Benchmark that measures cost-efficiency of LLMs on real software development tasks",
  "main": "dist/cli.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/cli.js",
    "bench": "node dist/cli.js run",
    "dev": "ts-node src/cli.ts",
    "setup": "bash scripts/setup-fixtures.sh",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "bin": {
    "llm-dev-bench": "dist/cli.js"
  },
  "dependencies": {
    "@anthropic-ai/vertex-sdk": "^0.4.0",
    "@anthropic-ai/sdk": "^0.36.0",
    "@google/generative-ai": "^0.21.0",
    "chart.js": "^4.5.1",
    "chartjs-node-canvas": "^5.0.0",
    "commander": "^12.1.0",
    "dotenv": "^16.4.5",
    "js-yaml": "^4.1.0",
    "openai": "^4.77.0",
    "p-limit": "^3.1.0",
    "pdfkit": "^0.17.2"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.10.0",
    "@types/pdfkit": "^0.17.5",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.2"
  }
}
